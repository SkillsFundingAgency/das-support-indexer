<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Sfa.Das.Console.Web.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="Sfa.Das.Console.Web" vmsize="small">
    <Runtime executionContext="elevated" />
    <Startup>
      <Task commandLine="StartUp\IIS.bat" executionContext="elevated" taskType="background">
      </Task>
    </Startup>
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
          <Binding name="Endpoint2" endpointName="Endpoint2" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
    
      <Setting name="EmpUserApiBaseUrl" />
      <Setting name="EmpUserApiClientId" />
      <Setting name="EmpUserApiClientSecret" />
      <Setting name="EmpUserApiIdentifierUri" />
      <Setting name="EmpUserApiTenant" />
      <Setting name="EmpUserApiCertificateThumbprint" />

      <Setting name="AccountApiBaseUrl" />
      <Setting name="AccountApiClientId" />
      <Setting name="AccountApiClientSecret" />
      <Setting name="AccountApiIdentifierUri" />
      <Setting name="AccountApiTenant" />
      <Setting name="AccountApiCertificateThumbprint" />

      <Setting name="LevyApiBaseUrl" />
      <Setting name="LevyApiClientId" />
      <Setting name="LevyApiClientSecret" />
      <Setting name="LevyApiIdentifierUri" />
      <Setting name="LevyApiTenant" />
      <Setting name="LevyTokenCertificate" />
      <Setting name="LevyHttpClientBaseUrl" />
      
      <Setting name="LoggingRedisConnectionString" />
      <Setting name="EnvironmentName"/>
      <Setting name="ida_ADFSMetadata" />
      <Setting name="ida_Wtrealm" />
      <Setting name="ida_GroupClaim"/>
      <Setting name="ida_Tier2Claim"/>
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="CryptoSalt" />
      <Setting name="CryptoSecret" />
      <Setting name="ChallengeTimeout" />

      <Setting name="Support_Azure_Search_QueryKey" />
      <Setting name="Support_Azure_Search_ServiceName" />
      <Setting name="Support_SubSite_0" />
      <Setting name="Support_SubSite_1" />

      <Setting name="InstrumentationKey"/>
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="https" port="443" certificate="WebSslCert"/>
      <InputEndpoint name="Endpoint2" protocol="http" port="80"/>
    </Endpoints>
    <Certificates>
      <Certificate name="WebSslCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>