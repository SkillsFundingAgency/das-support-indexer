@model SFA.DAS.Support.Portal.Web.ViewModels.HeaderViewModel

<header role="banner" id="global-header" class="with-proposition">
    <div class="header-wrapper">
        <div class="header-global">
            <div class="header-logo">
                <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
                    <img src="/govuk_template/assets/images/gov.uk_logotype_crown_invert_trans.png?0.23.0" width="36" height="32" alt=""> GOV.UK
                </a>
            </div>

        </div>
        <div class="header-proposition">
            <div class="content">
                <a href="#proposition-links" class="js-header-toggle menu">Menu</a>
                <nav id="proposition-menu">
                    <a href="/" id="proposition-name">DAS Support</a>
                    <ul id="proposition-links">
                        @Html.Partial("_LoginPartial")
                    </ul>
                </nav>
            </div>
        </div>
    </div>
</header>
<div id="global-header-bar"></div>