@model SFA.DAS.Support.Portal.Web.ViewModels.HeaderViewModel

<header role="banner" id="global-header" class="with-proposition">
    <div class="header-wrapper">
        <div class="header-global">
            <div class="header-logo">
                <a href="/" title="Go to the DAS support portal homepage" id="logo" class="content">
                    <img src="/assets/images/gov.uk_logotype_crown_invert_trans.png?0.23.0" width="36" height="32" alt=""> GOV.UK
                </a>
            </div>

        </div>
        <div class="header-proposition">
            <div class="content">
                <nav id="proposition-menu">
                    <a href="/" id="proposition-name">DAS Support</a>
                    <p id="proposition-link">
                        @Html.Partial("_LoginPartial")
                    </p>
                </nav>
            </div>
        </div>
    </div>
</header>
<div id="global-header-bar"></div>